<style>

    /* Make the image fully responsive */
    .carousel-inner img {
        width: 100%;
        height: 100%;
    }
    .mobileView {
        display: none;
    }
    .middleView {
        display: none;
    }
    .gridDiv {
        display: grid;
        grid-column-gap: 10px;
        grid-template-columns: auto auto auto auto auto auto;
        padding: 10px;
    }
    .gridDiv div {
        height: 200px;
        margin: 20px;
        box-shadow: 5px 5px 25px rgba(0, 0, 0, 0.12);
    }
    .carousel-indicators li {
            background-color: #E9EBED;
            width: 16px;
            height: 14px;
            border-radius: 50%;
            margin: 1%;
    }
    .carousel-indicators {
        bottom: -100px;
    }
    .carousel-control-prev-icon {
        background-image: url("${URLUtils.staticURL('/images/Vector1.svg')}");
    }
    .carousel-control-next-icon {
        background-image: url("${URLUtils.staticURL('/images/Vector2.svg')}");
    }
    .carousel-control-next-icon, .carousel-control-prev-icon {
        height: 40px;
        width: 40px;
    }
    .carousel-control-next, .carousel-control-prev {
        width: unset;
        top: -40px;
    }
    .carousel-inner {
        width: 90%;
    }
    .container {
        /* width: 100% !important; */
        padding-top: 100px;
        padding-bottom: 100px;
    }


    /*CSS For Swipper content */
    .swiper-container {
      width: 100%;
      height: 300px;
      padding-top: 60px;
      padding-bottom: 60px;
      margin-bottom: 20px;
    }
    .swiper-pagination-bullet {
        width: 14px;
        height: 14px;
        background: #E9EBED;
    }

    html,
    body {
      position: relative;
      height: 100%;
    }

    body {
      background: #eee;
      font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
      font-size: 14px;
      color: #000;
      margin: 0;
      padding: 0;
    }

    .swiper-slide {
      text-align: center;
      padding: 90px;
      font-size: 18px;
      background: #fff;
      margin-right: 20px !important;
      /* margin-left:20px;
      margin-right:20px; */
      box-shadow: 5px 5px 25px rgb(0 0 0 / 12%);

      /* Center slide text vertically */
      display: -webkit-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      -webkit-justify-content: center;
      justify-content: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      -webkit-align-items: center;
      align-items: center;
    }


    @media only screen and (max-width: 1080px) {
        .gridDiv {
            grid-template-columns: auto auto auto;
        }
        .middleView {
            display: block;
        }
        .fullView {
            display: none;
        }
        .container {
            padding-bottom: 115px;
        }
        .carousel-indicators {
            bottom: -70px;
        }
    }
    @media only screen and (max-width: 1024px) {
        .container {
            padding-top: 50px;
        }
    }
    @media only screen and (max-width: 700px) {
        .mobileView {
            display: block;
        }
        .container {
            display: none;
        }
    }
    @media only screen and (min-width: 700px) {
        .container {
            max-width: 2000px;
        }
    }
</style>
